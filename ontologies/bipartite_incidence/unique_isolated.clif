
(cl-text http://colore.oor.net/bipartite_incidence/unique_isolated.clif

(cl-imports http://colore.oor.net/bipartite_incidence/weak_bipartite.clif)

(forall (x y)
        (if     (and    (point x)
                        (point y)
                        (not (= x y)))
                (exists (l)
                        (and    (line l)
                                (in x l)))))

)
