(cl-text http://colore.oor.net/containment/c_1.clif)

(cl-imports http://colore.oor.net/containment/containment_def.clif)

(cl-comment "no partial overlaps")

(forall (x y)
        (if     (containmentOverlaps x y)
                (or     (containedIn x y)
                        (properContainedIn y x)))
                        
)
