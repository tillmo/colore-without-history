
(cl-text http://colore.oor.net/mereology/overlap_ext.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(cl-imports http://colore.oor.net/mereology/definitions/mereology_def.clif)

(forall (x y)
        (if     (not (= x y))
                (exists (z)
                        (iff    (overlaps z x)
                                (disjoint z y)))))

)
