
(cl-text http://colore.oor.net/mereology/sum_overlap.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(cl-imports http://colore.oor.net/mereology/definitions/mereology_def.clif)

(cl-comment "Axiom SA15 from Simons")

(forall (x y w z)
	(if	(and	(sum x y z)
			(overlaps z w))
		(or	(overlaps z x)
			(overlaps z y))))

)
