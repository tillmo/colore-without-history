
(cl-text http://colore.oor.net/multigraphs/unique_isolated_vertex.clif

(cl-imports http://colore.oor.net/multigraphs/pseudograph.clif)

(forall (x y)
        (if     (and    (vertex x)
                        (vertex y)
                        (not (= x y)))
                (exists (e z)
                        (madj e x z))))

)
