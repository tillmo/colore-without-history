
(cl-text http://colore.oor.net/orderings/negatively_transitive.clif

(cl-imports http://colore.oor.net/orderings/partial_ordering.clif)

(cl-imports http://colore.oor.net/orderings/definitions/orderings_def.clif)

(forall (x y z)
	(if	(and	(incomparable x y)
			(incomparable y z))
		(incomparable x z)))

)
