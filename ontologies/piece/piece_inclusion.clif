(cl-text http://colore.oor.net/piece/piece_inclusion.clif)
(cl-imports http://colore.oor.net/piece/piece_def.clif)

(cl-text "Inclusion Space")

(forall (x y)
        (if     (not (pieceOf x y))
                (exists (z)
                        (and    (properPieceOf z x)
                                (not (properPieceOf z y))))

)
