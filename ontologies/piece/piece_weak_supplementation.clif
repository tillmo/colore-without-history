(cl-text http://colore.oor.net/piece/piece_weak_supplementation.clif)
(cl-imports http://colore.oor.net/piece/piece_def.clif)

(cl-text "Weak Supplementation Principle")

(forall (x y)
        (if     (properPieceOf x y)
                (exists (z)
                        (and    (properPieceOf z y)
                                (pieceDisjoints z x))))

)
