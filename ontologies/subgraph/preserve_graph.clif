(cl-text http://colore.oor.net/subgraph/preserve_graph.clif

(cl-imports http://colore.oor.net/subgraph/subgraph_root.clif)

(cl-imports http://colore.oor.net/bipartite_incidence/parallel_lines.clif)

(forall (x y l1 l2)
	(if	(and	(adj x y)
			(point x)
			(point y)
			(line l1)
			(line l2)
			(in x l1)
			(in y l2))
		(adj l1 l2)))

)
