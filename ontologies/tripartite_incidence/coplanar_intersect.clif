
(cl-text http://colore.oor.net/tripartite_incidence/coplanar_intersect.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (p l1 l2 q)
	(if	(and	(point p)
			(line l1)
			(line l2)
			(plane q)
			(in p q)
			(in p l1)
			(in p l2)
			(in l1 q))
		(in l2 q)))

)
