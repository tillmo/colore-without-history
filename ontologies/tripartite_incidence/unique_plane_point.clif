
(cl-text http://colore.oor.net/tripartite_incidence/unique_plane_point.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (q1 q2 p1 p2)
	(if	(and	(plane q1)
			(plane q2)
			(point p1)
			(point p2)
			(in p1 q)
			(in p2 q))
		(or	(= p1 p2)
			(= q1 q2))))

)
