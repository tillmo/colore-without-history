
(cl-text http://colore.oor.net/tripartite_incidence/weak_nonisolated_line.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (l q)
	(if	(and	(line l)
			(in l q)
			(plane q))
		(exists (p q2)
			(and	(point p)
				(plane q2)
				(in p q2)
				(in p l)))))

)
