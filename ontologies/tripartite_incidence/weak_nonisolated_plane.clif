
(cl-text http://colore.oor.net/tripartite_incidence/weak_nonisolated_plane.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (q p)
	(if	(and	(plane q)
			(point p)
			(in p q))
		(exists (l p2)
			(and	(line l)
				(point p2)
				(in p2 l)
				(in l q)))))

)
