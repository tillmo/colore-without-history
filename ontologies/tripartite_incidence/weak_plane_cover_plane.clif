
(cl-text http://colore.oor.net/tripartite_incidence/weak_plane_cover_plane.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (p l q)
	(if	(and	(plane q)
			(line l)
			(point p)
			(in p l)
			(in p q))
		(exists (q2)
			(and	(in l q2)
				(plane q2)))))

)
